<h2>This is where your category form partial will go, to be rendered in new and edit</h2>

<%= form_for(category) do |f| %>
  <div>
    <%= f.label :name %>:
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>

<% @category.errors.each do |error_array, error| %>
  <p><%= flash[:error] = error %><p>
<% end %>