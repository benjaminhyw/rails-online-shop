<div class="item-container">
  <span class="page-header"><h1>Categories</h1></span>

  <div class="individual-category-container">
    <% @categories.each do |category| %>
      <div class="individual-category">
        <p><%= link_to category.name, category_path(category) %></p>
      </div>
    <% end %>
  </div>
</div>